{
  "name": "petfeeder",
  "version": "1.1",
  "dependencies": ["uart", "sensor", "api", "time"],
  "services": [
    {
      "name": "feed_pet",
      "params": {
        "portions": "int"
      }
    },
    {
      "name": "test_message",
      "params": {
        "target": "int",
        "source": "int",
        "command": "int",
        "value": "int"
      }
    },
    {
      "name": "set_feeding_schedule",
      "params": {
        "schedules": [
          {
            "hour": "int",
            "minute": "int",
            "portions": "int"
          }
        ]
      }
    },
    {
      "name": "clear_feeding_schedules",
      "params": {}
    }
  ]
}
